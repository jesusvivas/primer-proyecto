---
interface Props {
    title: string
    price: number
}

const {title,price} = Astro.props
---

<div>
    <div>
        <h3>{title}</h3>
        <p>${price}/mes</p>
    </div>
    <div>
        <button>Selecionar Plan Básico</button>
    </div>
    <div>
        <ul>
            <li>Acceso a todas las áreas del gimnasio</li>
            <li>Clases grupales incluidas</li>
        </ul>
    </div>
</div>