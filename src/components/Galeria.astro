---
interface Props{
    image: string
}

const {image} = Astro.props
---

<div class="container">
    <div class="img">
        <img src={image} alt="">
    </div>


<style>
    img{
        border-radius: 5px;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
    }
</style>